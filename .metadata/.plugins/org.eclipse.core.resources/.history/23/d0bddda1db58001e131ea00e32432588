import lombok.RequiredArgsConstructor;
import lombok.Setter;

@Component
@Setter 
@RequiredArgsConstructor
public class FinalProductDao {
	
	public void product() {
		System.out.println("---- ProductDao product() 메소드 ----");
		System.out.println(" 내가 마지막 프로덕트 입니다. ");
	}
}
